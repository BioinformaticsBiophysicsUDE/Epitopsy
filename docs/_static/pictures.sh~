
awk < pdf2jpg.log 'BEGIN { ORS="" } { print "convert -background white -alpha deactivate -quality 80 -density " $2 " -scale " $3 "% " $1 ".pdf " $1 ".jpg" ; printf("%c", "") }' | xargs --null -n 1 echo

